<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="infectionGraph" packages="">
    <Appenders>
        <File name="File" fileName="application.log">
            <PatternLayout pattern="%d %p %c{1.} [%t] %m%n" />
        </File>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="File" />
        </Root>
    </Loggers>
</Configuration>